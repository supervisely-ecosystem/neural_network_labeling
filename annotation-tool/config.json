{
  "name": "NN Image Labeling",
  "type": "app",
  "categories": [
    "neural network",
    "labeling"
  ],
  "description": "Use deployed neural network in labeling interface",
  "docker_image": "supervisely/base-py-sdk:6.1.79",
  "main_script": "annotation-tool/src/main.py",
  "gui_template": "annotation-tool/src/gui.html",
  "task_location": "application_sessions",
  "isolate": true,
  "headless": false,
  "integrated_into": [
    "image_annotation_tool"
  ],
  "icon": "https://i.imgur.com/nnB2BqW.png",
  "icon_background": "#FFFFFF",
  "hotkeys": [
    {
      "hotkey": "ctrl+m",
      "command": "inference"
    }
  ],
  "poster": "https://i.imgur.com/AFv8KQa.png"
}